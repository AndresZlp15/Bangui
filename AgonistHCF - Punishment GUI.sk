command /spam [<offline player>]:
	permission: {@permission}
	permission message: {@permission.message}
	trigger:
		if arg-1 is not set:
			send "&5&lAgonistHCF &8» &7Please specify a player."
			stop
		if arg-1 is set:
			open chest with 3 row named "&5&lAgonistHCF &8» &7Punishments" to player
			wait 2 ticks
			format slot 11 of player with green clay named "&a&lFirst Offence" with lore "&8» &5Mute 30 Minutes" to close then run [execute console command "/sudo force %player% litebans:mute %arg-1% 30m Spam (First Offence) -s"]
			format slot 13 of player with yellow clay named "&e&lSecond Offence" with lore "&8» &5Mute 1 Hour" to close then run [execute console command "/sudo force %player% litebans:mute %arg-1% 1h Spam (Second Offence) -s"]
			format slot 15 of player with red clay named "&c&lThird Offence" with lore "&8» &5Mute 2 Hours" to close then run [execute console command "/sudo force %player% litebans:mute %arg-1% 2h Spam (Third Offence) -s"]
			format slot 18 of player with paper named "&5&lSee history" with lore "&7See the history of the player||&7to determine the offence number" to close then run [make player execute command "/history %arg-1%"]
			format slot 26 of player with arrow named "&5&lBack" with lore "&7Back to the main menu" to run [make player execute command "/punish %arg-1%"]
			stop

