options:

	permission: hcf.command.punish
	permission.message: &cYou don't have permissions.
	
command /punish [<offline player>]:
	permission: {@permission}
	permission message: {@permission.message}
	trigger:
		if arg-1 is not set:
			send "&5&lAgonistHCF &8» &7Please specify a player."
			stop
		if arg-1 is set:
			open chest with 6 row named "&5&lAgonistHCF &8» &7Punishments" to player
			wait 1 ticks
			format slot 10 of player with book named "&5&lSpam" to run make player execute command "/spam %arg-1%"
